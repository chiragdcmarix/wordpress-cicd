.inner-col{
    color: #fff;
    text-align: center;
    height: 400px;
    max-width: 400px;
    border-radius: 56px;
    margin: 0 auto;
    display: flex;
    padding: 20px;
    align-items: center;
    justify-content: center;
    position: relative;
    margin-bottom: 40px;
    margin-top: 40px;
offset-block-end: ;}
.inner-col:after{
    content: '';
    position: absolute;
    background-color: #ffffff96;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    border-radius: 56px;
}


.highlighted-terms {
    color: var(--color-primary) !important;
}

.inner-col:before{
    content: '';
    position: absolute;
    background-color: var(--color-default);
    bottom: 0;
    left: 0;
    right: 0;
    border-radius: 56px;
    transition: all ease .5s;
    height: 0;
    opacity: 1;
    z-index: 9;
}
.inner-col:hover:before{
    height: 100%;
}
.inner-col i{
    font-size: 70px;
    margin-bottom: 10px;
}
.inner-col h3{
    font-size: 30px;
    position: relative;
    z-index: 9;
    color: #000;
}
.inner-col:hover h3{
    color: #fff;
}
@media only screen and (max-width:991px){
    .inner-col{
        margin-bottom: 0;
