/* review page css */

.section.hero-banner-section {
    height: 65vh;
    align-items: flex-end;
    display: flex;
    position: relative;
}

.padding-main {
    padding: 100px 0 100px;
}

.top-wrap,
.overall-rating {
    display: flex;
    justify-content: space-between;
}

.img-wrap {
    width: 130px;
    height: 130px;
    border-radius: 50%;
    overflow: hidden;
}

.img-wrap img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.overall-rating {
    margin-top: 50px;
    margin-bottom: 20px;
    font-weight: 600;
}

.desc {
    font-size: 20px;
    margin: 10px 0 20px;
}

h2.title {
    font-weight: 600;
    color: var(--color-default);
}

#review-bottom h4 {
    margin: 15px 0;
    font-weight: 600;
}

.card {
    box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px;
    border: none;
    border-radius: 20px;
    margin-bottom: 30px;
    padding: 20px 20px 0;
}

.date {
    color: gray;
    font-size: 16px;
    font-weight: 600;
}

.review-img {
    width: 100%;
    height: 500px;
    overflow: hidden;
}

.review-img img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.rating-over i {
    font-size: 24px;
    line-height: 24px;
    margin-left: 10px;
    margin-bottom: 5px;
    color: var(--color-primary);
}

span.rating-over {
    display: flex;
    align-items: center;
}

a.page-link,
.page-item:last-child .page-link,
.page-item:first-child .page-link {
    color: var(--color-default);
    border: none;
    border-radius: 50%;
    width: 50px;
    height: 50px;
    text-align: center;
    line-height: 42px;
    box-shadow: rgba(60, 64, 67, 0.3) 0px 1px 2px 0px, rgba(60, 64, 67, 0.15) 0px 2px 6px 2px;
}

li.page-item {
    margin: 0 10px;
}

/* review page css ends*/

/* gallery page css */
.gallery-box {
    box-shadow: rgba(0, 0, 0, 0.1) 0px 1px 3px 0px, rgba(0, 0, 0, 0.06) 0px 1px 2px;
    padding-bottom: 10px;
}
.gallery-card{
    max-height: 300px;
    height: 300px;
    width: 100%;
}
.gallery-wrap img {
    max-width: 100%;
    height: 100%;
    width: 100%;
    object-fit: cover;
}

.gallery-desc {
    color: var(--text-color);
    position: relative;
    z-index: 99;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    line-height: 26px;
    font-weight: 600;
    padding: 0 10px;
    margin: 10px 0 0;
}
/* gallery page css ends*/


/* blog page css */
.blog-card {
    margin-bottom: 30px;
    background-color: var(--color-secondary);
    box-shadow: rgba(0, 0, 0, 0.24) 0px 3px 8px;
    border-radius: 10px;
}

.blog-card .img {
    display: block;
}

.blog-card .img img {
    max-width: 100%;
}

.blog-card .content {
    padding: 20px;
}

.blog-card .content ul {
    list-style-type: none;
    margin: 0 0 10px;
    padding: 0;
}

.blog-card .content ul li {
    display: inline-block;
    margin-right: 30px;
}

.blog-card .content ul li i {
    color: var(--color-default);
    margin-right: 5px;
    position: relative;
    top: -1px;
}

.blog-card .content ul li a {
    color: var(--text-color);
    display: inline-block;
    font-size: 18px;
    font-weight: 600;
}


.blog-card .content h3 a {
    color: var(--color-default);
    text-decoration: none;
    font-weight: 700;
    position: relative;
    z-index: 99;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 5px;
    font-size: 22px;
}

.blog-card .blog-desc {
    color: var(--text-color);
    position: relative;
    z-index: 99;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 10px;
    font-size: 14px;
}

.content .button-outline {
    color: var(--color-secondary);
    background: var(--color-default);
    border: 1px solid var(--color-default);

}

/* blog page css ends*/

/* blog detail page css*/


.single-blog .blog-content {
    padding: 30px 15px 0px;
    width: 100%;
    transform: translateX(0);
    margin-top: 0px;
}

.single-blog .blog-content .title {
    font-size: 36px;
    /* line-height: 30px; */
}

.single-blog .blog-content p {
    font-size: 16px;
    line-height: 30px;
    margin-top: 20px;
}

.blog-details-content .blog-quote {
    background-color: none;
    position: relative;
    margin-left: 50px;
    margin-top: 40px;
}

.blog-details-content .blog-quote::after {
    content: "";
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    background: var(--color-default);
    width: 6px;
    height: 105px;
}

@media only screen and (max-width: 575px) {
    .blog-details-content .blog-quote::after {
        display: none;
    }
}

.blog-details-content .blog-quote .blockquote {
    padding-right: 125px;
    padding-left: 25px;
    margin-bottom: 0;
    position: relative;
}

@media only screen and (max-width: 1199px) {
    .blog-details-content .blog-quote .blockquote {
        padding-right: 0;
    }
}

@media only screen and (max-width: 575px) {
    .blog-details-content .blog-quote .blockquote {
        margin-left: 0;
    }
}

.blog-details-content .blog-quote .blockquote:not(:first-child) {
    margin-top: 25px;
}

.blog-details-content .blog-quote .blockquote:not(:last-child) {
    margin-bottom: 25px;
}

.blog-details-content .blog-quote .blockquote p {
    display: inline;
    font-size: 18px;
    color: #333333;
    line-height: 32px;
    font-weight: 600;
    margin-top: 0;
}

.blog-details-content .blog-details-text p {
    font-size: 16px;
    line-height: 30px;
    margin-top: 50px;
    padding: 0 30px 00px;
}

.single-blog .blog-image .top-meta {
    position: absolute;
    top: 35px;
    left: 30px;
}

.single-blog .blog-image {
    position: relative;
    overflow: hidden;
    border-radius: 10px;
}

.single-blog .blog-image .top-meta .date {
    width: 65px;
    height: 65px;
    background: linear-gradient(-180deg, #43baff 0%, var(--color-default) 100%);
    color: #fff;
    font-size: 18px;
    line-height: 47px;
    border-radius: 5px;
    display: inline-block;
    text-align: center;
    padding: 10px;
}

.sidebar-widget .search-form {
    position: relative;
}


.sidebar-widget .search-form input:not([type="submit"]):not([type="checkbox"]):not([type="radio"]):not([type="file"]) {
    width: 100%;
    height: 50px;
    margin-bottom: 0;
    border: 0;
    padding-right: 35px;
    padding-left: 15px;
    background: #f2f4f7;
    color: var(--text-color);
    font-weight: 700;
    outline: none;
    overflow: hidden;
}

.sidebar-widget .search-form button {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 0;
    width: 50px;
    height: 50px;
    background: transparent;
    border: 0;
    border-radius: 5px;
    font-size: 14px;
    color: #111111;
    transition: all 0.3s linear;
}

.sidebar-widget .search-form button:hover {
    background: var(--color-default);
    color: #ffffff;
}

.sidebar-widget .widget-title {
    margin-bottom: 25px;
    background: var(--color-default);
    color: var(--color-secondary);
    padding: 5px 15px;
}

.sidebar-widget .widget-title .title {
    font-size: 20px;
    font-weight: 700;
    display: inline-block;
}

.blog-meta {
    margin-bottom: 15px;
}

.single-blog .blog-content .blog-meta span {
    font-size: 16px;
    line-height: 24px;
    font-weight: 500;
    margin-right: 15px;
    margin-bottom: 20px;
    color: var(--color-default);
}

.sidebar-widget .recent-posts ul li {
    border-top: 1px solid #e5e5e5;
    margin-top: 15px;
    list-style: none;
    padding: 15px 12px 0;
}

.blog-sidebar {
    box-shadow: -1px 1px 12px -8px;
    padding-bottom: 10px;
}
.sticky-sidebar{
    position: sticky;
    top: 9px;   
}
.sidebar-widget .recent-posts ul {
    padding-left: 0;
}

.sidebar-widget {
    margin-bottom: 25px;
}

.sidebar-widget .recent-posts ul li:first-child {
    border-top: 0;
    padding-top: 0;
    margin-top: 0;
}

.post-link {
    display: flex;
}

.post-link .post-thumb {
    margin-right: 20px;
    width: 50px;
    height: 50px;
}

.post-link .post-thumb img {
    border-radius: 50%;
    width: 100%;
    height: 100%;
}

.post-link .post-text {
    flex: 1;
}

.post-link .post-text .title {
    font-size: 16px;
    line-height: 18px;
    font-weight: 600;
    transition: all 0.3s linear;
    color: var(--color-default);
    padding-left: 20px;
    position: relative;
}

.post-link .post-text .title i {
    position: absolute;
    left: 0;
}

.post-link .post-text .post-meta {
    font-size: 12px;
    font-weight: 400;
    line-height: 24px;
    color: #838383;
    display: inline-block;
    margin-top: 5px;
}

.post-link .post-text .post-meta i {
    color: var(--color-default);
    margin-right: 5px;
}

.post-link:hover .post-text .title {
    color: var(--color-default);
}

/* blog detail page css ends*/


/* contact page css */


.back-contact-page .back-title-sec h2 {
    font-size: 50px;
    font-weight: 700;
    color: var(--color-default);
    margin: 0px;
}

.back-contact-page .back-title-sec p {
    font-size: 18px;
    color: #55575c;
}

.back-contact-page .blog-form {
    z-index: 9;
    position: relative;
    padding-top: 15px;
}

.back-contact-page .blog-form .back-input {
    position: relative;
}

.back-contact-page .blog-form .back-input img {
    position: absolute;
    bottom: 55px;
    left: 30px;
}

.back-contact-page .blog-form .back-textarea img {
    position: absolute;
    top: 57px;
    left: 47px;
    z-index: 1;
}

